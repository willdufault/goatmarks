{
    "name": "goatmarks",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite build && node server.js",
        "start": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "css-build": "node-sass --omit-source-map-url ./static/sass/bulma.scss ./static/css/bulma.css",
        "css-watch": "npm run css-build -- --watch"
    },
    "devDependencies": {
        "@iconify/svelte": "^3.1.6",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/adapter-static": "^3.0.1",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "bulma": "^0.9.4",
        "node-sass": "^9.0.0",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.3"
    },
    "type": "module",
    "dependencies": {
        "@sveltejs/adapter-node": "^3.0.0",
        "@types/is-url": "^1.2.32",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "mongoose": "^8.0.4",
        "muuri": "^0.9.5"
    }
}
